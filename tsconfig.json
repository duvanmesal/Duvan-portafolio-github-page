{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "types": ["astro/client", "react"]
  },
  "include": [".astro/types.d.ts", "src", "components", "app", "src/data"],
  "exclude": ["dist"]
}
